    let allVideos = [];

    // å…¨å±€ Hls å®ä¾‹ï¼ˆæ¨¡æ€æ¡†å…³é—­æˆ–åˆ‡æ¢è§†é¢‘æ—¶éœ€è¦é”€æ¯ï¼‰
    let hlsInstance = null;

    // æ’­æ”¾è§†é¢‘ï¼ˆBunny Player ç‰ˆæœ¬ï¼‰
    let modalPlayer = null;
    async function playVideo(video) {
      const modalEl = document.getElementById('videoModal');
      const modal = new bootstrap.Modal(modalEl);
      const containerId = '#modalVideoContainer';

      const modalTitle = document.getElementById('videoModalTitle');
      const modalVideoTitle = document.getElementById('modalVideoTitle');
      const modalVideoViews = document.getElementById('modalVideoViews');
      const modalVideoDate = document.getElementById('modalVideoDate');

      modalTitle.textContent = video.title || 'è§†é¢‘æ’­æ”¾';
      modalVideoTitle.textContent = video.title || 'æœªçŸ¥è§†é¢‘';
      modalVideoViews.textContent = video.views || 0;
      modalVideoDate.textContent = formatRelativeTime(video.createdAt);

      try {
        console.log('å‡†å¤‡æ’­æ”¾è§†é¢‘:', video);
        
        if (modalPlayer) {
          modalPlayer.destroy();
          modalPlayer = null;
        }

        const container = document.querySelector(containerId);
        container.innerHTML = '';

        // é€šè¿‡å—æ§ API è·å–æ’­æ”¾ URLï¼ˆå¸¦ UA/æƒé™æ ¡éªŒï¼‰
        let playUrl = null;
        try {
          const resp = await fetch(`/api/video/${video._id}/preview`);
          if (resp.ok) {
            const j = await resp.json();
            playUrl = j && j.data && j.data.url;
          }
        } catch(e) { console.warn('preview api failed', e); }

        if (!playUrl) {
          throw new Error('æ— æ³•è·å–è§†é¢‘æ’­æ”¾åœ°å€');
        }

        console.log('ä½¿ç”¨æ’­æ”¾åœ°å€:', playUrl);

        if (playUrl.includes('iframe.mediadelivery.net')) {
          // Bunny iframe æ’­æ”¾å™¨
          const iframe = document.createElement('iframe');
          iframe.src = playUrl;
          iframe.style.position = 'absolute';
          iframe.style.top = '0';
          iframe.style.left = '0';
          iframe.style.width = '100%';
          iframe.style.height = '100%';
          iframe.frameBorder = '0';
          iframe.allowFullscreen = true;
          
          container.appendChild(iframe);
        } else if (playUrl.includes('.m3u8')) {
          // HLS æµåª’ä½“æ’­æ”¾
          const video = document.createElement('video');
          video.controls = true;
          video.style.width = '100%';
          video.style.height = '100%';
          video.style.position = 'absolute';
          video.style.top = '0';
          video.style.left = '0';
          video.style.background = 'transparent';
          video.style.backgroundColor = 'transparent';
          
          if (Hls.isSupported()) {
            if (hlsInstance) {
              hlsInstance.destroy();
            }
            hlsInstance = new Hls();
            hlsInstance.loadSource(playUrl);
            hlsInstance.attachMedia(video);
            hlsInstance.on(Hls.Events.MANIFEST_PARSED, function() {
              video.play();
            });
          } else if (video.canPlayType('application/vnd.apple.mpegurl')) {
            video.src = playUrl;
            video.addEventListener('loadedmetadata', function() {
              video.play();
            });
          }
          
          container.appendChild(video);
        } else {
          // æ™®é€šè§†é¢‘æ–‡ä»¶
          const video = document.createElement('video');
          video.src = playUrl;
          video.controls = true;
          video.style.width = '100%';
          video.style.height = '100%';
          video.style.position = 'absolute';
          video.style.top = '0';
          video.style.left = '0';
          video.style.background = 'transparent';
          video.style.backgroundColor = 'transparent';
          video.autoplay = true;
          
          container.appendChild(video);
        }

        modal.show();
      } catch (error) {
        console.error('è§†é¢‘æ’­æ”¾é”™è¯¯:', error);
        alert('è§†é¢‘æ’­æ”¾å¤±è´¥: ' + error.message);
      }
    }

    // åŠ¨æ€è®¾ç½®è§†é¢‘å®¹å™¨å®½é«˜æ¯”
    function setVideoAspectRatio(element, ratio) {
      if (!element) return;
      
      // æ”¯æŒå¤šç§æ ¼å¼ï¼š
      // setVideoAspectRatio(el, '16/9')
      // setVideoAspectRatio(el, 1.777)
      // setVideoAspectRatio(el, { width: 16, height: 9 })
      
      let aspectRatio;
      if (typeof ratio === 'string') {
        aspectRatio = ratio;
      } else if (typeof ratio === 'number') {
        aspectRatio = ratio.toString();
      } else if (typeof ratio === 'object' && ratio.width && ratio.height) {
        aspectRatio = `${ratio.width}/${ratio.height}`;
      } else {
        aspectRatio = '16/9'; // é»˜è®¤å€¼
      }
      
      element.style.setProperty('--video-aspect-ratio', aspectRatio);
    }
    
    // æ‰¹é‡è®¾ç½®è§†é¢‘å®½é«˜æ¯”
    function setAllVideoAspectRatio(ratio) {
      document.querySelectorAll('.video-wrapper, .video-thumbnail').forEach(el => {
        setVideoAspectRatio(el, ratio);
      });
    }
    
    // æ ¹æ®è§†é¢‘æ•°æ®è‡ªåŠ¨è®¾ç½®å®½é«˜æ¯”ï¼ˆå°†æ¥å¯æ‰©å±•ï¼‰
    function autoSetAspectRatio(videoElement, videoData) {
      if (!videoElement || !videoData) return;
      
      // å¦‚æœè§†é¢‘æ•°æ®åŒ…å«å®½é«˜ä¿¡æ¯ï¼Œè®¡ç®—å®½é«˜æ¯”
      if (videoData.width && videoData.height) {
        const ratio = videoData.width / videoData.height;
        setVideoAspectRatio(videoElement, ratio);
      }
      // å¦åˆ™ä½¿ç”¨é»˜è®¤çš„16:9
    }

    // æœç´¢åŠŸèƒ½
    function handleSearch(event) {
      event.preventDefault();
      const query = document.getElementById('searchInput').value.trim();
      if (query) {
        // å®ç°æœç´¢é€»è¾‘
        window.location.href = `/search?q=${encodeURIComponent(query)}`;
